{
  "name": "kanvas-studio",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn run build:env && yarn run flow && node server.js",
    "build": "yarn run build:env && yarn run flow && next build",
    "start": "yarn run flow && cross-env NODE_ENV=production node server.js",
    "siege": "siege -c255 -t1M http://localhost:4424",
    "set:env": "bash bin/set.env.sh",
    "flow-typed": "flow-typed",
    "install": "npx flow-typed install styled-components@^4.0.0",
    "build:env": "node bin/parse.env.js .env > env.config.json"
  },
  "dependencies": {
    "aws-sdk": "^2.594.0",
    "body-parser": "^1.19.0",
    "device": "^0.3.9",
    "dotenv": "^8.1.0",
    "express": "^4.14.0",
    "helmet": "^3.21.2",
    "next": "latest",
    "prop-types": "^15.7.2",
    "rate-limiter-flexible": "^1.1.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-use": "^12.2.2",
    "ssr-window": "^1.0.1",
    "styled-components": "^4.0.0",
    "throttle-debounce": "^2.1.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.7.4",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-transform-define": "^2.0.0",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.114.0"
  }
}
